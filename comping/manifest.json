{
  "manifest_version": 2,
  "name": "Comping",
  "version": "0.1.2",
  "description": "An add-on to accompany your browsing by adding notes on works cached from Sonemic websites (music, movies, video games).",
  "browser_specific_settings": {
    "gecko": {
      "id": "comping@dbeley.ovh",
      "strict_min_version": "91.0"
    }
  },
  "permissions": [
    "storage",
    "tabs",
    "http://rateyourmusic.com/*",
    "https://rateyourmusic.com/*",
    "https://www.rateyourmusic.com/*",
    "http://www.rateyourmusic.com/*",
    "https://rym.com/*",
    "http://rym.com/*",
    "https://glitchwave.com/*",
    "http://glitchwave.com/*",
    "https://www.glitchwave.com/*",
    "http://www.glitchwave.com/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Comping",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://rateyourmusic.com/*",
        "http://rateyourmusic.com/*",
        "https://www.rateyourmusic.com/*",
        "http://www.rateyourmusic.com/*",
        "https://glitchwave.com/*",
        "http://glitchwave.com/*",
        "https://www.glitchwave.com/*",
        "http://www.glitchwave.com/*"
      ],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/async-utils.js",
        "shared/dom-utils.js",
        "content/data-sync.js",
        "content/data-extract.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "http://navidrome.docker-era.home/*",
        "http://*/navidrome/*",
        "https://*/navidrome/*"
      ],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/navidrome-overlay.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://store.steampowered.com/*", "http://store.steampowered.com/*"],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/steam-overlay.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "http://jellyfin.docker-era.home/*",
        "http://*/jellyfin/*",
        "https://*/jellyfin/*"
      ],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/jellyfin-overlay.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://www.last.fm/*", "http://www.last.fm/*"],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/lastfm-overlay.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://www.youtube.com/*",
        "http://www.youtube.com/*",
        "https://m.youtube.com/*",
        "http://m.youtube.com/*",
        "https://music.youtube.com/*"
      ],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/youtube-overlay.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://www.humblebundle.com/*",
        "http://www.humblebundle.com/*",
        "https://humblebundle.com/*",
        "http://humblebundle.com/*"
      ],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/humble-overlay.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://open.spotify.com/*"],
      "js": [
        "shared/normalize.js",
        "shared/config.js",
        "shared/runtime-utils.js",
        "shared/debug-utils.js",
        "shared/dom-utils.js",
        "shared/observer-utils.js",
        "shared/badge-utils.js",
        "shared/match-utils.js",
        "content/spotify-overlay.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "content/data-sync.js",
    "shared/normalize.js",
    "shared/config.js",
    "shared/async-utils.js",
    "shared/runtime-utils.js",
    "shared/debug-utils.js",
    "shared/dom-utils.js",
    "shared/observer-utils.js",
    "shared/badge-utils.js",
    "shared/match-utils.js"
  ]
}
